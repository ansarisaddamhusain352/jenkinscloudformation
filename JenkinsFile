pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh "export AWS_DEFAULT_REGION=us-east-1"
				sh "aws cloudformation create-stack --stack-name myteststack --template-body file://config.json --region 'us-east-1'"
            }
        }
        stage('Test') {
            steps {
                sh "ls"
            }
        }
        stage('Deploy') {
            steps {
                sh "ls"
            }
        }
    }
}